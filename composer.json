{
    "name": "jtad009/cakephp-paystack",
    "description": "Paystack plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.3.2 <4.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "PayStack\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PayStack\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "minimum-stability": "dev",
    "license": "proprietary",
    "authors": [
      {
        "name": "Israel Edet",
        "email": "jtad009@gmail.com"
      }
    ]
}
